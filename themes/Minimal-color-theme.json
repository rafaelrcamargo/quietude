  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#565552"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#938f82",
        "fontStyle": ""
      }
    },
    {
      "name": "String Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#57554f",
        "fontStyle": ""
      }
    },
    {
      "name": "String Regular Expressions",
      "scope": ["string.regexp", "punctuation.definition.string.regexp"],
      "settings": {
        "foreground": "#57554f",
        "fontStyle": ""
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#bab39c"
      }
    },
    {
      "name": "Booleans",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#bab39c"
      }
    },
    {
      "name": "Built-in constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#bab39c"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#73716b"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#73716b"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#6c765c"
      }
    },
    {
      "name": "Types, Classes, Structs, Enums",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.enum",
        "entity.other.inherited-class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#ca9e66"
      }
    },
    {
      "name": "Variables, Properties, Parameters",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#bab39c"
      }
    },
    {
      "name": "Special Variables (this, self, super)",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#bab39c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Attributes (HTML/XML tags)",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#bab39c"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#57554f"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ca9e66"
      }
    },
    {
      "name": "Constructors",
      "scope": [
        "entity.name.function.constructor",
        "punctuation.definition.constructor"
      ],
      "settings": {
        "foreground": "#ca9e66"
      }
    },
    {
      "name": "Preprocessor directives",
      "scope": [
        "keyword.control.directive",
        "punctuation.definition.directive"
      ],
      "settings": {
        "foreground": "#ca9e66"
      }
    },
    {
      "name": "Labels",
      "scope": ["entity.name.label"],
      "settings": {
        "foreground": "#6578a6"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "punctuation.definition.heading"],
      "settings": {
        "foreground": "#ca9e66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "foreground": "#ca9e66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "foreground": "#ca9e66",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.title",
        "markup.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#6c765c"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": ["markup.underline.link", "meta.link.inline"],
      "settings": {
        "foreground": "#73716b"
      }
    },
    {
      "name": "Markdown Code Blocks/Inline",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown",
        "punctuation.definition.markdown"
      ],
      "settings": {
        "foreground": "#938f82",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List Markers",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#57554f"
      }
    }
  ]
}
